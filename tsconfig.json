{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es6",
      "noImplicitAny": true,
      "sourceMap": true,
      "outDir": "dist",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "exclude": [
        "node_modules",
    ],
    "include": [
        "src/**/*"
    ]
  }